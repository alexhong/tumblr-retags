{
   "name": "Tumblr Retags",
   "description": "Adds tags to reblog notes, and wraps all tag lists for easy readability.",
   "version": "0.2",
   "manifest_version": 2,
   "content_scripts": [ {
      "include_globs": [ "*://www.tumblr.com/*" ],
      "js": [ "tumblr-retags.user.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_idle"
   } ]
}
