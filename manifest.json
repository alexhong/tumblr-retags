{
   "name": "Tumblr Retags",
   "description": "Shows tags added to reblogs, and wraps all tag lists for easy readability.",
   "version": "0.1",
   "manifest_version": 2,
   "content_scripts": [ {
      "exclude_globs": [  ],
      "exclude_matches": [  ],
      "include_globs": [ "*://www.tumblr.com/*" ],
      "js": [ "tumblr-retags.user.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_idle"
   } ]
}
