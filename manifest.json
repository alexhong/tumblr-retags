{
   "manifest_version": 2,
   "name": "Tumblr Retags",
   "version": "0.6.7",
   "description": "Adds tags to reblog notes, and wraps all tags for readability.",
   "icons": { 
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png" 
   },
   "content_scripts": [ {
      "matches": [ "*://*.tumblr.com/*" ],
      "include_globs": [ "*://www.tumblr.com/*" ],
      "js": [ "jquery-2.0.3.min.js", "tumblr-retags.user.js" ]
   } ]
}